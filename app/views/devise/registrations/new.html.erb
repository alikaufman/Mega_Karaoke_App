<div class="container">
<div class="loginform">
<h2 class="signin">Sign up</h2>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <div class="row">
    <div><%= f.text_field :first_name, autofocus: true, placeholder: 'First Name', class: "col-xs-8 col-xs-offset-2 fname input-lg" %></div>
  </div>
  <div class="row">
    <div><%= f.text_field :last_name, placeholder: 'Last Name', class: "col-xs-8 col-xs-offset-2 input-lg" %></div>
  </div>
  <div class="row">
    <div><%= f.email_field :email, placeholder: 'Email Address', class: "col-xs-8 col-xs-offset-2 input-lg" %></div>
  </div>
  <div class="row">
    <div><%= f.password_field :password, autocomplete: "off", placeholder: 'Password', class: "col-xs-8 col-xs-offset-2 input-lg" %></div>
  </div>
  <div class="row">
    <div><%= f.password_field :password_confirmation, autocomplete: "off", placeholder: 'Password Confirmation', class: "col-xs-8 col-xs-offset-2 input-lg" %></div>
  </div>
  <div class="subform"><%= f.submit "Sign up", class: 'btn' %></div>
<% end %>

<div class="text-center"><%= render "devise/shared/links" %></div>
</div>
</div>