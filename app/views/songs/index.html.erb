<table class="table">
  <tr>
    <th class="marmelad"><%= sortable "artist" %></th>
    <th class="marmelad"><%= sortable "title" %></th>
    <th class="marmelad"><%= sortable "genre" %></th>
  </tr>
  <% for song in @songs %>
  <% if song.user_id == current_user.id %>
  <tr>
    <td class="marmelad"><%= song.artist %></td>
    <td class="marmelad song_link">"<%= link_to song.title, song %>"</td>
    <td class="marmelad"><%= song.genre %></td>
  </tr>
  <% end %>
  <% end %>
</table>
<br>






